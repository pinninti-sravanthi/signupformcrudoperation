	</servlet>
	<servlet-mapping>
		<servlet-name>SignUpFormServlet</servlet-name>
		<url-pattern>/SignUpFormServlet</url-pattern>
	</servlet-mapping>

	<servlet-mapping>
		<servlet-name>SignUpFormServlet2</servlet-name>
		<url-pattern>/SignUpFormServlet2</url-pattern>
	</servlet-mapping>
<servlet-mapping>
		<servlet-name>delete</servlet-name>
