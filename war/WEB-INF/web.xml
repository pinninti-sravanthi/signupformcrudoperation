http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
	<servlet>
		<servlet-name>SignUpFormServlet</servlet-name>
		<servlet-class>com.example.SignUpForm.SignUpFormServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>SignUpFormServlet2</servlet-name>
