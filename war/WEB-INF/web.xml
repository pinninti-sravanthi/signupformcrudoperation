	</servlet-mapping>
<servlet-mapping>
		<servlet-name>update</servlet-name>
		<url-pattern>/update</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>Login</servlet-name>
		<jsp-file>/index.jsp</jsp-file>
	</servlet>

	<servlet-mapping>
		<servlet-name>Login</servlet-name>
		<url-pattern>/login</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>Delete1</servlet-name>
		<jsp-file>/delete1.jsp</jsp-file>
	</servlet>

	<servlet-mapping>
		<servlet-name>Delete1</servlet-name>
		<url-pattern>/delete1</url-pattern>
	</servlet-mapping>

	
